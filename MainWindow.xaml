                    <Border Grid.Row="1" >
                        <StackPanel Grid.Row="0" Orientation="Horizontal">
                            <TextBlock  Grid.Row="0" Margin="0, 0, 6, 0"><Bold>Slave TM221CE40R</Bold></TextBlock>
                            <Button  Grid.Row="0" Name="Btplc40on" Click="ConnectTM40" Width="55" Background="DodgerBlue">Connect</Button>
                            <Button  Grid.Row="0" Name="Btplc40off" Click="DisconnectTM40" Width="80" Background="DodgerBlue">Disconnect</Button>
                        </StackPanel>
                    </Border>
                    <!--Row 2 ++++++++++++++++++++++++++++++++++ Control Eventer 1 and Pumb 2 ++++++++++++++++-->
                    <Border Grid.Row="2" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Grid.Row="0"><Bold>Eventer 1</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="0" Name="BtEventer1on" Click="Eventer_1_On" Background="red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="0" Name="BtEventer1off" Click="Eventer_1_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="1"><Bold>Clean Water Pumb 1</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="1" Name="BtPumb1on" Click="Pumb_1_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="1" Name="BtPumb1off" Click="Pumb_1_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="2"><Bold>Frequency</Bold></TextBlock>
                            <Slider Name="FrequencyPumb1" Grid.Column="1" Grid.Row="2" Orientation="Horizontal"
                                HorizontalAlignment="Center" Minimum="30" Maximum="50" Width="50"
                                TickPlacement="BottomRight" TickFrequency="1" IsSnapToTickEnabled="True" 
                                Value="{Binding Path=MW1, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBlock Name="FrequencyPumb1Block1" Grid.Column="2" Grid.Row="2" Text="{Binding MW1}" TextAlignment="Center" Width="20"/>
                            <TextBlock Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right">hz</TextBlock>
                        </Grid>
                    </Border>
                    <!--Row 3 ++++++++++++++++++++++++++++++++++ Control Eventer 2 and Pumb 2 ++++++++++++++++-->
                    <Border Grid.Row="3" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Grid.Row="0"><Bold>Eventer 2</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="0" Name="BtEventer2on" Click="Eventer_2_On" Background="red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="0" Name="BtEventer2off" Click="Eventer_2_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="1" VerticalAlignment="Center"><Bold>Clean Water Pumb 2</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="1" Name="BtPumb2on" Click="Pumb_2_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="1" Name="BtPumb2off" Click="Pumb_2_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Row="2" Grid.Column="0" ><Bold>Frequency</Bold> (Hz)</TextBlock>
                            <Slider Name="FrequencyPumb2" Grid.Column="1" Grid.Row="2" Orientation="Horizontal"
                                HorizontalAlignment="Center" Minimum="30" Maximum="50" Width="50"
                                TickPlacement="BottomRight" TickFrequency="1" IsSnapToTickEnabled="True" 
                                Value="{Binding Path=MW2, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBlock Name="FrequencyPumb1Block2" Grid.Column="2" Grid.Row="2" Text="{Binding MW2}" TextAlignment="Center" Width="20"/>
                            <TextBlock Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right">hz</TextBlock>
                        </Grid>
                    </Border>
                    <!--Row 4 ++++++++++++++++++++++++++++++++++ Control Pumb 3~6 ++++++++++++++++++++++++++++-->
                    <Border Grid.Row="4" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Column="0" Grid.Row="0"><Bold>Pumb Javen 3</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="0" Name="BtPumb3on" Click="Pumb_Javen_3_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="0" Name="BtPumb3off" Click="Pumb_Javen_3_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="1" ><Bold>Pumb Javen 4</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="1" Name="BtPumb4on" Click="Pumb_Javen_4_On" Background="Red" >Start</Button>
                            <Button Grid.Column="2" Grid.Row="1" Name="BtPumb4off" Click="Pumb_Javen_4_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="2" ><Bold>Stirring Pumb 5</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="2" Name="BtPumb5on" Click="Pumb_Khuay_5_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="2" Name="BtPumb5off" Click="Pumb_Khuay_5_Off" Background="Green">Stop</Button>
                            <TextBlock Grid.Column="0" Grid.Row="3" ><Bold>Stirring Pumb 6</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="3" Name="BtPumb6on" Click="Pumb_Khuay_6_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="3" Name="BtPumb6off" Click="Pumb_Khuay_6_Off" Background="Green">Stop</Button>
                        </Grid>
                    </Border>
                    <!-- Row 5++++++++++++++++++++++++++++++++++ status +++++++++++++++++++++++++++++++++++++-->
                    <Border Grid.Row="5" Background="Gray">
                        <Grid Background="Gray" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <Style TargetType="{x:Type TextBlock}">
                                    <Setter Property="Margin" Value="3, 0, 0, 0"/>
                                    <Setter Property="HorizontalAlignment" Value="Left"/>
                                    <Setter Property="FontSize" Value="12"/>
                                    <Setter Property="Margin" Value="0, 0 , 5, 0" />
                                </Style>
                                <Style TargetType="{x:Type Rectangle}">
                                    <Setter Property="Canvas.Right" Value="20"/>
                                    <Setter Property="Canvas.Top" Value="3"/>
                                    <Setter Property="Height" Value="12"/>
                                    <Setter Property="Width" Value="12"/>
                                </Style>
                                <Style TargetType="{x:Type DockPanel}">
                                    <Setter Property="VerticalAlignment" Value="Center" />
                                </Style>
                            </Grid.Resources>
                            <DockPanel Grid.Row="0" Name="InputPersure">
                                <TextBlock><Bold>Persure Pipe</Bold></TextBlock>
                                <TextBlock Text="{Binding Path=MW4, StringFormat={}{0:0.0}}" HorizontalAlignment="Right" Margin="0, 0, -150, 0"/>
                                <TextBlock HorizontalAlignment="Right">bar</TextBlock>
                            </DockPanel>
                            <DockPanel Grid.Row="1">
                                <TextBlock><Bold>Hight water tank 1</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle >
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="AnimationHightWaterTank"/>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                            <DockPanel Grid.Row="2">
                                <TextBlock><Bold>Low water tank 1</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle>
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="AnimationLowtWaterTank" />
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                            <DockPanel Grid.Row="3">
                                <TextBlock><Bold>Error phase station 1</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle>
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="AnimationErrorPhaseStation2"/>
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                        </Grid>
                    </Border>
                    <!--Row 6 ++++++++++++++++++++++++++++++++++ Mode Auto ++++++++++++++++++++++++++++++-->
                    <Border Grid.Row="6" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Grid.Row="0" Grid.Column="0" ><Bold>Mode Auto</Bold></TextBlock>
                            <Button Grid.Row="0" Grid.Column="1"  Name="BtAutoOn" Click="AutoTM40On" Background="Red">On</Button>
                            <Button Grid.Row="0" Grid.Column="2"  Name="BtAutoOff" Click="AutoTM40Off" Background="Green">Off</Button>
                            <TextBlock Grid.Row="1" Grid.Column="0" ><Bold>Pressure</Bold></TextBlock>
                            <Slider Grid.Row="1" Grid.Column ="1" Name="PersureSet"              
                                Orientation="Horizontal" HorizontalAlignment="Center"
                                Minimum="3" Maximum="5" Width="50"
                                TickPlacement="BottomRight" TickFrequency="0.2" IsSnapToTickEnabled="True"
                                Value="{Binding Path=MW3, UpdateSourceTrigger=PropertyChanged}"/>
                            <TextBlock x:Name="PersureSetBlock" Grid.Row="2" Grid.Column="2" Text="{Binding MW3}" TextAlignment="Left" Width="20" />
                            <TextBlock Grid.Row="2" Grid.Column="2" HorizontalAlignment="Right">bar</TextBlock>
                        </Grid>
                    </Border>
                    <!-- Row 7 ++++++++++++++++++++++++++++++++++ title +++++++++++++++++++++++++++++++++++++ -->
                    <Border Grid.Row="7" Background="DarkCyan" BorderBrush="DarkGray" BorderThickness="1" >
                        <TextBlock FontSize="15" Foreground="Black" HorizontalAlignment="Center"><Bold>Status Panel Station 2</Bold></TextBlock>
                    </Border>
                    <!--Row 8 ++++++++++++++++++++++++++++++++++ Connect TM221CE24R ++++++++++++++++++++++++++-->
                    <Border Grid.Row="8" >
                        <StackPanel Grid.Row="0" Orientation="Horizontal">
                            <TextBlock  Grid.Row="0" Margin="0, 0, 6, 0"><Bold>Slave TM221CE24R</Bold></TextBlock>
                            <Button  Grid.Row="0" Name="Btplc24on"  Click="ConnectTM24" Width="55" Background="DodgerBlue">Connect</Button>
                            <Button  Grid.Row="0" Name="Btplc24off" Click="DisconnectTM24" Width="80" Background="DodgerBlue">Disconnect</Button>
                        </StackPanel>
                    </Border>
                    <!--Row 9 ++++++++++++++++++++++++++++++++++ Control tram 1 ++++++++++++++++++++++++++++++-->
                    <Border Grid.Row="9" >
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <!--Raw water pumb 1-->
                            <TextBlock Grid.Column="0" Grid.Row="0"><Bold>Raw Water Pumb 1</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="0" Name="Btnt1on" Click="Pumb_NT_1_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="0" Name="Btnt1off" Click="Pumb_NT_1_Off" Background="Green">Stop</Button>
                            <!--Raw water pumb 2-->
                            <TextBlock Grid.Column="0" Grid.Row="1" ><Bold>Raw Water Pumb 2</Bold></TextBlock>
                            <Button Grid.Column="1" Grid.Row="1" Name="Btnt2on" Click="Pumb_NT_2_On" Background="Red">Start</Button>
                            <Button Grid.Column="2" Grid.Row="1" Name="Btnt2off" Click="Pumb_NT_2_Off" Background="Green">Stop</Button>
                        </Grid>
                    </Border>
                    <!-- Row 10 ++++++++++++++++++++++++++++++++++ Status ++++++++++++++++++++++++++++++++++++-->
                    <Border Grid.Row="10" Background="Gray">
                        <Grid Background="Gray" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Grid.Resources>
                                <Style TargetType="{x:Type TextBlock}">
                                    <Setter Property="Margin" Value="3, 0, 0, 0"/>
                                    <Setter Property="HorizontalAlignment" Value="Left"/>
                                    <Setter Property="FontSize" Value="12"/>
                                </Style>
                                <Style TargetType="{x:Type Rectangle}">
                                    <Setter Property="Canvas.Right" Value="20"/>
                                    <Setter Property="Canvas.Top" Value="3"/>
                                    <Setter Property="Height" Value="12"/>
                                    <Setter Property="Width" Value="12"/>
                                </Style>
                                <Style TargetType="{x:Type DockPanel}">
                                    <Setter Property="VerticalAlignment" Value="Center" />
                                </Style>
                            </Grid.Resources>
                            <DockPanel Grid.Row="0">
                                <TextBlock><Bold>Hight water</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle>
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="HightWaterRawAnimation" />
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                            <DockPanel Grid.Row="1">
                                <TextBlock><Bold>Low Water</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle>
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="AnimationLowWater" />
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                            <DockPanel Grid.Row="2">
                                <TextBlock><Bold>Error Phase</Bold></TextBlock>
                                <Canvas>
                                    <Rectangle>
                                        <Rectangle.Fill>
                                            <SolidColorBrush x:Name="ErrorPhaseAnimation" />
                                        </Rectangle.Fill>
                                    </Rectangle>
                                </Canvas>
                            </DockPanel>
                        </Grid>
                    </Border>
                </Grid>
            </Border>
        </DockPanel>

    </Border>

</Window>
